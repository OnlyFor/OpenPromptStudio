{
  "version": 2,
  "env": {
  },
  "builds": [
    {
      "src": "server/index.ts",
      "use": "@vercel/node",
      "config": { "includeFiles": ["server/**"] }
    }
  ],
  "routes": [
    {
      "src": "/prompt-studio/(.*)",
      "dest": "server/index.ts"
    }
  ],
  "rewrites": [{ "source": "/(.*)", "destination": "/index.html" }]
}
