{
  "version": 2,
  "env": {
  },
  "framework": "vite",
  "buildCommand": "vercel build",
  "outputDirectory": "/vercel/path0/dist",
  "builds": [
    {
      "src": "server/index.ts",
      "use": "@vercel/node",
      "config": { "includeFiles": ["server/**"] }
    }
  ],
  "routes": [
    {
      "src": "/prompt-studio/translate/prompts/(.*)",
      "dest": "server/index.ts"
    }
  ]

}
