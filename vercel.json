{
  "version": 2,
  "env": {
  },
  "builds": [
    {
      "src": "server/index.ts",
      "use": "@unjs/jiti",
      "config": { "includeFiles": ["server/**"] }
    },
    {
      "src": "package.json",
      "use": "@vercel/static-build"
    },
    {
      "src": "/data/build.js",
      "use": "@vercel/node",
      "config": { "includeFiles": ["data/**"] }
    }
  ],
  "routes": [
    {
      "src": "/prompt-studio/translate/prompts",
      "dest": "server/index.ts"
    }
  ]
}
