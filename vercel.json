{
  "version": 2,
  "env": {
  },
  "installCommand": "npm install",
  "regions": ["sfo1"],
  "builds": [
    {
      "src": "server/index.ts",
      "use": "jiti"
    },
    {
      "src": "package.json",
      "use": "@vercel/static-build"
    },
    {
      "src": "/data/build.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/prompt-studio/translate/(.*)",
      "dest": "server/index.ts"
    },
    {
      "src": "/localPromptDefineMap.json",
      "dest": "/data/build.js"
    }
  ]
}
