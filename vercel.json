{
  "version": 2,
  "env": {
  },
  "installCommand": "npm install",
  "regions": ["sfo1"],
  "builds": [
    {
      "src": "api/index.ts",
      "use": "https://github.com/unjs/jiti/tree/v1"
    },
    {
      "src": "package.json",
      "use": "@vercel/static-build"
    },
    {
      "src": "/data/build.js",
      "use": "@vercel/node"
    }
  ],
  "rewrites": [{ "source": "/prompt-studio/translate/prompts", "destination": "api/index.ts" }]
}
