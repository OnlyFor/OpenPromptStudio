{
  "version": 2,
  "env": {
  },
  "builds": [
    {
      "src": "server/index.ts",
      "use": "@vercel/node",
      "config": { "includeFiles": ["server/**"] }
    },
    {
      "src": "vite.config.ts",
      "use": "@vercel/static-build",
      "config": { "includeFiles": ["src/**", "web/**"] }
    }
  ],
  "routes": [
    {
      "src": "/prompt-studio/translate/prompts/(.*)",
      "dest": "server/index.ts"
    },
    {
      "src": "/(.*)",
      "dest": "vite.config.ts"
    }
  ]
}
